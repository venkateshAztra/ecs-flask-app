{
  "family": "ecs-demo-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::352029560899:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ecs-demo",
      "image": "352029560899.dkr.ecr.us-east-1.amazonaws.com/ecs-demo:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
