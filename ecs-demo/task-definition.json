{
  "family": "ecs-demo-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::352029560899:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::352029560899:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ecs-demo",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-demo",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
